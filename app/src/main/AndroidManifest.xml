<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kaczmarek.bggapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.BggApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BggApplication">
        <activity android:name=".application.activities.RankHistoryActivity"></activity>
        <activity android:name=".application.activities.UpdateBoardGameActivity" />
        <activity android:name=".application.activities.InsertBoardGameActivity" />
        <activity android:name=".application.activities.ImportGameActivity" />
        <activity android:name=".application.activities.LocationDetailsActivity" />
        <activity android:name=".application.activities.LocationListActivity" />
        <activity android:name=".application.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>